{
  "globalRateLimit": 100,
  "requestsPerMinute": 60,
  "burstLimit": 10,
  "cooldownPeriod": 300,
  "maxRetries": 3,
  "retryDelay": 5000,
  "exponentialBackoff": true,
  "retryOnErrors": [
    "TIMEOUT",
    "CONNECTION_ERROR",
    "RATE_LIMITED"
  ],
  "maxConcurrentJobs": 5,
  "jobTimeout": 300000,
  "memoryLimit": 1024,
  "diskSpaceThreshold": 85,
  "dataRetentionDays": 90,
  "autoCleanup": true,
  "compressOldData": true,
  "backupEnabled": true,
  "emailNotifications": true,
  "slackNotifications": false,
  "webhookNotifications": false,
  "notificationThresholds": {
    "errorRate": 10.0,
    "successRate": 80.0,
    "queueSize": 100
  },
  "healthCheckInterval": 30,
  "metricsRetention": 30,
  "logLevel": "info",
  "enableDebugMode": false,
  "apiKeyRotation": true,
  "encryptData": true,
  "auditLogging": true,
  "ipWhitelist": []
}